{
  "name": "apsconnect/connect-sdk-migration-framework",
  "description": "Small middleware to ease the service migration from legacy to Connect.",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Unay Santisteban",
      "email": "david.santisteban@ingrammicro.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-json": "*",
    "apsconnect/connect-sdk": "*"
  },
  "autoload": {
    "psr-4": {
      "Connect\\Middleware\\Migration\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "mockery/mockery": "^1.2"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
